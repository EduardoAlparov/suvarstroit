                                <div class="simple-range__input-wrapper">
                                    <input class="simple-range__input styled-slider slider-progress" type="range"
                                        required="required" name="cost" id="costInput" min="4000000" max="15000000"
                                        value="5689000">
                                </div>
                                <div class="simple-range__values">
                                    <div class="input simple-range__input-value simple-range__input-value--text-18">
                                        <output data-calc-output for="cost" id="costOutput">
                                            5 689 000
                                        </output><span>&nbsp;</span><span class="simple-range__span-currency">₽</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="form-calc__input">
                        <div class="simple-range simple-range--prepayment">
                            <div class="simple-range__wrapper">
                                <div class="simple-range__label simple-range__label--text-14">Стоимость квартиры</div>

                                <div class="simple-range__input-wrapper">
                                    <input class="simple-range__input styled-slider slider-progress" type="range"
                                        required="required" name="prepayment" id="prepaymentInput" min="1000000"
                                        max="5000000" value="2345000">
                                </div>

                                <div class="simple-range__values">
                                    <div class="input simple-range__input-value simple-range__input-value--text-18">
                                        <output data-calc-output for="prepayment" id="prepaymentOutput">
                                            2 345 000
                                        </output><span>&nbsp;</span><span class="simple-range__span-currency">₽</span>
                                    </div>
                                    <div class="simple-range__input-percentege">
                                        <span class="simple-range__percentege-value">20</span><span>&nbsp;</span><span
                                            class="simple-range__percentege-currency">&#x25;</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="form-calc__input">
                        <div class="select select--blue">
