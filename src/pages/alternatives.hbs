                                <option value="Ипотека для IT специалистов">Ипотека для IT специалистов</option>
                                <option value="Ипотека с господдержкой">Ипотека с господдержкой</option>
                                <option value="Губернаторская ипотека">Губернаторская ипотека</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-calc__input">
                        <div class="simple-range simple-range--term">
