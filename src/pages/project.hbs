<section class="home home--fillscreen-project section section--minus-mb js-home-swiper-wrapper">
    <div class="home-slider home-slider--fullscreen home-slider--small">
        <div class="home-slider__image-swiper-wrapper">
            <div class="home-slider__image-swiper swiper">
                <ul class="home-slider__image-swiper-wrapper swiper-wrapper">
                    <li class="home-slider__swiper-slide swiper-slide image-block">
                        <picture>
                            <source type="image/webp" srcset="./img/home/bigger.webp" alt="home image" />
                            <img class="lazy" src="./img/home/bigger.jpg" alt="home image" />
                        </picture>
                    </li>
                    <li class="home-slider__swiper-slide swiper-slide image-block">
                        <picture>
                            <source type="image/webp" srcset="./img/home/bigger.webp" alt="home image" />
                            <img class="lazy" src="./img/home/bigger.jpg" alt="home image" />
                        </picture>
                    </li>
                </ul>

                <div class="swiper-pagination"></div>
            </div>
        </div>

        <div class="home-slider__body js-home-slider-body">
            <div class="cursor home-slider__swiper-btn">
                <svg class="icon">
                    <use xlink:href="#swiper-arrow"></use>
                </svg>
            </div>

            <div class="home-slider__site-container">
                <div class="home-slider__info-swiper-slide">
                    <div class="home-slider__info-heading">
                        <p class="home-slider__address project-card__address">
                            Лаишевский район
                        </p>
                        <h1 class="home-slider__info-title home-slider__info-title--text-biggest">
                            Южный парк
                        </h1>
                    </div>

                    <ul class="home-slider__tabs">
                        <li class="home-slider__tab tab">
                            <svg class="icon">
                                <use xlink:href="#key"></use>
                            </svg>
                            <span class="tab__text tab__text--text-14">Комфорт</span>
                        </li>
                        <li class="home-slider__tab tab tab--icon">
                            <svg class="icon">
                                <use xlink:href="#key"></use>
                            </svg>
                            <span class="tab__text tab__text--text-14">Сдача в 2026</span>
                        </li>
                        <li class="home-slider__tab tab">
                            <svg class="icon">
                                <use xlink:href="#key"></use>
                            </svg>
                            <span class="tab__text tab__text--text-14">Ипотека от 6%</span>
                        </li>
